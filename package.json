{
  "name": "eslint-config-brunodsazevedo",
  "version": "1.0.0-beta.0",
  "private": false,
  "description": "ESLint configuration used by brunodsazevedo.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsc && node fix-imports.mjs",
    "build:cleanup": "find dist -name '*.d.ts' -exec sed -i.bak 's/\\.ts/\\.js/g' {} + && find dist -name '*.bak' -delete && find dist -name '*.js' -exec sed -i.bak 's|from \"\\./\\([^\"]*\\)\";|from \"./\\1.js\";|g' {} + && find dist -name '*.bak' -delete",
    "test": "npm run type-check && npm run test:build",
    "test:build": "npm run build && node test-build.mjs",
    "prepublishOnly": "npm run build",
    "postpublish": "git push --tags"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "brunodsazevedo/eslint-config-dotazevedo"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": ">=5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/js": "^9.35.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "react": "^19.1.1",
    "typescript": "^5.9.2"
  }
}
